{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/server.js"
      },
      {
        "src": "/api/auth",
        "dest": "/server.js"
      },
      {
        "src": "/api/test",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
